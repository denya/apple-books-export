{
  "name": "apple-books-export",
  "version": "1.0.1",
  "description": "Export highlights, bookmarks, and notes from Apple Books",
  "type": "module",
  "bin": {
    "apple-books-export": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "export": "bun run src/index.ts",
    "dev": "bun run src/index.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:local": "npm run build && node ./dist/index.js --help",
    "test:bun": "npm run build && bun ./dist/index.js --help",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "apple-books",
    "export",
    "highlights",
    "notes",
    "bookmarks",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/denya/apple-books-export.git"
  },
  "homepage": "https://github.com/denya/apple-books-export#readme",
  "bugs": {
    "url": "https://github.com/denya/apple-books-export/issues"
  },
  "dependencies": {},
  "optionalDependencies": {
    "better-sqlite3": "^11.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
